{
  "name": "dg-astro",
  "type": "module",
  "version": "0.0.0-development",
  "description": "Astro based exploration for dg",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/dgattey/dg-astro.git"
  },
  "author": "Dylan Gattey <hi@dylangattey.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/dgattey/dg-astro/issues"
  },
  "homepage": "https://github.com/dgattey/dg-astro#readme",
  "scripts": {
    "prepare": "husky install",
    "dev": "astro dev",
    "start": "astro dev",
    "build": "astro build --experimental-integrations",
    "preview": "wrangler pages dev ./dist",
    "astro": "astro",
    "format": "prettier --write --plugin-search-dir=. .",
    "lint": "eslint . --cache --fix --ext .js,.ts,.astro",
    "lint:types": "tsc --noEmit --project ./tsconfig.json",
    "release": "semantic-release"
  },
  "dependencies": {
    "@astrojs/cloudflare": "3.0.0",
    "@astrojs/partytown": "1.0.1",
    "@astrojs/preact": "1.2.0",
    "@astrojs/prefetch": "0.0.8",
    "@astrojs/sitemap": "1.0.0",
    "astro": "1.4.7",
    "astro-imagetools": "0.6.11",
    "image-size": "1.0.2",
    "preact": "10.11.1",
    "preact-render-to-string": "5.2.5",
    "sharp": "0.31.1"
  },
  "devDependencies": {
    "@commitlint/cli": "17.1.2",
    "@commitlint/config-conventional": "17.1.0",
    "@semantic-release/changelog": "6.0.1",
    "@types/node": "16.11.7",
    "@typescript-eslint/eslint-plugin": "5.40.0",
    "@typescript-eslint/parser": "5.40.0",
    "astro-compress": "1.0.11",
    "astro-robots-txt": "0.3.10",
    "eslint": "8.25.0",
    "eslint-plugin-astro": "0.20.0",
    "eslint-plugin-jsx-a11y": "6.6.1",
    "husky": "8.0.1",
    "lint-staged": "13.0.3",
    "prettier": "2.7.1",
    "prettier-plugin-astro": "0.5.5",
    "semantic-release": "19.0.5",
    "typescript": "4.8.4",
    "wrangler": "2.1.11"
  },
  "engines": {
    "node": ">=16"
  },
  "packageManager": "pnpm@7.11.0"
}
